<h1>Pirates:</h1>
<div class="pirate">
<h2>Name: <%= @pirate.name %></h2><br>
<h2>Weight: <%= @pirate.weight %></h2><br>
<h2>Height: <%= @pirate.height %></h2><br>
</div><br>

<h1>Ships:</h1>
<% @ships.each do |ship| %>
<div class="ship">
<h3>Name: <%= ship.name %></h3><br>
<h3>Type: <%= ship.type %></h3><br>
<h3>Booty: <%= ship.booty %></h3><br>
</div><br>
<% end %>
